<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping package="priv.guochun.psmc.authentication.user.model">
	<class
		name="TabPerson"
		table="TAB_PERSON"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Uuid"
			type="string"
			column="UUID"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="PersonName"
			column="PERSON_NAME"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Sex"
			column="SEX"
			type="integer"
			not-null="false"
			length="2"
		/>
		<property
			name="Age"
			column="AGE"
			type="integer"
			not-null="false"
			length="3"
		/>
		<property
			name="Telephone"
			column="TELEPHONE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Email"
			column="EMAIL"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="AccUuid"
			column="ACC_UUID"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="CityId"
			column="CITY_ID"
			type="string"
			not-null="false"
			length="20"
		/>

	</class>	
</hibernate-mapping>