<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="priv.guochun.psmc.authentication.user.model">
	<class
		name="TabAccount"
		table="TAB_ACCOUNTS"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Uuid"
			type="string"
			column="UUID"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="AccountName"
			column="ACCOUNT_NAME"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="AccountPass"
			column="ACCOUNT_PASS"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="IsLocked"
			column="IS_LOCKED"
			type="string"
			not-null="false"
			length="10"
		/>


	</class>	
</hibernate-mapping>