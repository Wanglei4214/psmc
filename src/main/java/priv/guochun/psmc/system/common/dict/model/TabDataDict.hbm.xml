<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >


<hibernate-mapping package="priv.guochun.psmc.system.common.dict.model">
	<class
		name="TabDataDict"
		table="TAB_DATA_DICT"
	>
	
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Integer"
			column="ID"
		>
			<generator class="sequence">
				<param name="sequence">TAB_DATA_DICT_SEQ</param>
			</generator>
			
		</id>
		
		<property
			name="DictId"
			column="DICT_ID"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="DictName"
			column="DICT_NAME"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Remark"
			column="REMARK"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="DictType"
			column="DICT_TYPE"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="Ordernum"
			column="ORDERNUM"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="DictNo"
			column="DICT_NO"
			type="string"
			not-null="false"
			length="100"
		/>

	</class>	
</hibernate-mapping>